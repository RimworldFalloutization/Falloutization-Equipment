<?xml version="1.0" encoding="utf-8"?>
<ModMetaData>
    <packageId>Falloutization.Equipment</packageId>
    <author>Canenald</author>
    <name>Falloutization: Equipment</name>
    <description>A mod that removes RimWorld vanilla equipment that doesn't fit the Fallout setting.</description>
    <supportedVersions>
        <li>1.6</li>
    </supportedVersions>
    <modDependencies>
        <li>
            <packageId>Rick.FCP.Core.Tools</packageId>
            <displayName>FCP - Tools</displayName>
            <downloadUrl>https://github.com/FalloutCollaborationProject/FCP-Tools</downloadUrl>
        </li>
        <li>
            <packageId>Rick.FCP.BallisticWeapons</packageId>
            <displayName>FCP - Ballistic Weapons</displayName>
            <downloadUrl>https://github.com/FalloutCollaborationProject/FCP-Ballistic-Weapons</downloadUrl>
        </li>
        <li>
            <packageId>Rick.FCP.NCR</packageId>
            <displayName>FCP - New California Republic</displayName>
            <downloadUrl>https://github.com/FalloutCollaborationProject/FCP-NCR</downloadUrl>
        </li>
    </modDependencies>
    <loadAfter>
      <li>Ludeon.RimWorld</li>
      <li>Ludeon.RimWorld.Royalty</li>
      <li>Ludeon.RimWorld.Ideology</li>
      <li>Ludeon.RimWorld.Odyssey</li>
      <li>Ludeon.RimWorld.Biotech</li>
      <li>Ludeon.RimWorld.Anomaly</li>
      <li>Rick.FCP.Core.Tools</li>
      <li>Rick.FCP.Enclave</li>
      <li>Rick.FCP.Legion</li>
      <li>Rick.FCP.MeleeWeapons</li>
    </loadAfter>
    <modVersion>1.0</modVersion>
</ModMetaData>
